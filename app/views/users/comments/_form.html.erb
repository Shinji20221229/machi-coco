<% tweet.tweet_comments.each do |tweet_comment| %>
  <p><%= image_tag tweet_comment.user.get_profile_image, size: "100x100" %></p>
  <%= tweet_comment.comment %>
  <% if tweet_comment.user == current_user %>
    <%= link_to "削除", tweet_tweet_comment_path(tweet_comment.tweet, tweet_comment), method: :delete, remote: true, date: {confirm: "本当に削除しますか？"} %>
  <% end %>
<% end %>