<% if tweet.favorited_by?(current_user) %>
  <p>
    <%= link_to "いいね削除", tweet_favorites_path(tweet), method: :delete, remote: true %>
      <%#= tweet.favorites.count %>
    <%# end %>
  </p>
  <% else %>
  <p>
    <%= link_to "いいね", tweet_favorites_path(tweet_id: tweet), method: :post, remote: true %>
      <%#= tweet.favorites.count %>
    <%# end %>
  </p>
<% end %>